/* pages/myOrderDetail/myOrderDetail.wxss */
view{
  box-sizing: border-box;
}
.addList-header{
  width: 100%;
  height: 167rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #ddd;
}
.addList-content{
  width: 100%;
  position: relative;
}
.userInfo{
  font-size: 33rpx;
  color: #333333;
}
.address{
  font-size: 25rpx;
  color: #666666;
  margin-top: 20rpx;
}
.address image{
  width: 25rpx;
  height: 25rpx;
  margin-right: 10rpx;
}
.default-add{
  display: inline-block;
  width: 63rpx;
  height: 38rpx;
  background-color: #c9151e;
  color: #fff;
  font-size: 21rpx;
  text-align: center;
  line-height: 38rpx;
  margin-left: 9rpx;
  vertical-align: top;
}
.panel{
  width: 100%;
  background-color: #fff;
  margin-top: 24rpx;
  margin-bottom: 24rpx;
  box-sizing: border-box;
}
.panel-content{
  width: 100%;
  height: 196rpx;
  padding:25rpx 29rpx 24rpx 29rpx;
  box-sizing: border-box;
  position: relative;
}
.panel-content-img{
  width: 146rpx;
  height: 146rpx;
}
.panel-content-right{
  display: inline-block;
  margin-left: 20rpx;
  vertical-align: top;
  position: relative;
}
.panel-content-title{
  font-size: 33rpx;
  color: #000000;
  line-height: 33rpx; 
  text-overflow: ellipsis;
  width: 520rpx;
  white-space: nowrap;
  overflow: hidden;
}
.panel-content-subTitle{
  font-size: 21rpx;
  color: #999999;
  margin-top: 16rpx;
  /* margin-left: -4rpx; */
  line-height: 21rpx;
}
.panel-content-price{
  margin-top: 58rpx;
  font-size:25rpx;
  color: #f23030;
  line-height: 18rpx;
}
.panel-content-num{
  position: absolute;
  right: 55rpx;
  bottom: 18rpx;
  font-size:21rpx;
}

.panel .panel-container{
  border-top: 1rpx solid #ddd;
}
.panel-sec .panel-con-title{
  width: 100%;
  height: 68rpx;
  padding: 0 29rpx;
  font-size: 23rpx; 
  color: #333333;
  box-sizing: border-box;
  line-height: 68rpx;
}
.panel-sec .panel-content-title{
  width: 465rpx;
}
.panel-sec .panel-content-img{
  margin-left: 55rpx;
}
.panel-sec .panel-con-footer{
  width: 100%;
  height: 68rpx;
  padding: 0 53rpx 0 29rpx;
  font-size: 23rpx; 
  color: #333333;
  box-sizing: border-box;
  line-height: 68rpx;
  text-align: right;
}

.bill-cell-group{
  width: 100%;
  margin-top: 24rpx;
  padding: 34rpx 0 0 28rpx;
  background-color: #ffffff;
  border-top: 1rpx solid #dddddd;
  border-bottom: 1rpx solid #dddddd;
}
.bill-cell-group.bill-cell-footer{
  height:147rpx;
  margin-top:0;
  border-top: none;
}
.bill-cell{
  width: 100%;
  /* height: 89rpx; */
  margin-bottom: 25rpx;
   display: flex;
  align-items: center; 
  font-size: 25rpx;
  line-height: 33.334rpx;
  position: relative;
}
.bill-cell-left{
  color: #333333;
}
.bill-cell-right{
  /* margin-left: 74rpx; */
  color: #333333;
}
.bill-cell-btn{
  position: absolute;
  width: 81rpx;
  height: 37rpx;
  top: 0;
  right: 180rpx;
  background-color: #eee;
  border-radius: 7rpx;
  font-size: 20rpx;
  color: #666666;
  text-align: center;
  line-height: 37rpx;
}
.freight-cell{
  position: relative;
  width: 100%;
  height: 90rpx;
  padding: 32rpx 29rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #ddd;
  display: flex;
  align-items: center;
  font-size: 25rpx;
  color: #333333;
  margin-top: 25rpx;
}
.freight-cell-left{
  flex: 1;
}
.freight-cell-right{
  position: relative;
  text-align: right;
  color: #f23030;
}
.freight-cell-left.remark{
  margin-right: 42rpx;
  flex: none;
}
.freight-cell-right.remark{
  position: relative;
  text-align: left;
  color: #333333;
}
.freight-cell-count{
  width: 100%;
  text-align: right;
  font-size: 21rpx;
  color: #999999;
  margin-top: 13rpx;
}
.totalPrice-cell{
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx solid #ddd;
  background-color: #fff;
  padding: 32rpx 29rpx;
  margin-top: 25rpx;
}
.totalPrice-cell-con{
  display: flex;
  align-items: center;
  font-size: 25rpx;
}
.totalPrice-cell-left{
  flex: 1;
  color: #333333;
}
.totalPrice-cell-right{
  color: #f23030;
}
.totalPrice-cell-count{
  font-size: 21rpx;
  text-align: right;
  width: 100%;
  color: #999999;
  margin-top: 13rpx;
}
.bottom-box{
  width: 100%;
  height: 97rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.bottom-box-btn-left{
  width: 215rpx;
  height: 97rpx;
  font-size: 33rpx;
  color: #000;
  line-height: 97rpx;
  text-align: center;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
}
.bottom-box-btn{
  width: 215rpx;
  height: 97rpx;
  background-color: #c9151e;
  font-size: 33rpx;
  color: #fff;
  line-height: 97rpx;
  text-align: center;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
}