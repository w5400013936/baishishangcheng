/* pages/myBillDetail/myBillDetail.wxss */
view{
  box-sizing: border-box;
}
.bill-header{
  width: 100%;
	background-color: #ffffff;
	 border: solid 1rpx #dddddd; 
  padding-left: 19rpx;
}
.bill-header-title{
  font-size: 25rpx;
  color: #999999;
  margin-top: 41rpx;
}
.bill-header-money{
  font-size: 40rpx;
  color: #F23030;
  margin: 26rpx 0;
  /* line-height: 76rpx; */
}
.bill-cell-group{
  margin-top: 22rpx;
  background-color: #ffffff;
}
.bill-cell{
  width: 100%;
  height: 89rpx;
  padding: 32rpx 0 32rpx 26rpx;
  border-bottom: 1rpx solid #dddddd;
  display: flex;
  align-items: center;
  font-size: 25rpx;
}
.bill-cell:nth-last-of-type(1){
  border-bottom:none;
}
.bill-cell-left{
  color: #333333;
}
.bill-cell-right{
  margin-left: 35rpx;
}